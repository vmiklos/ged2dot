[ged2dot]
# Input GEDCOM file.
input = Alice-Bob.ged
# Ouptut Graphviz file.
output = test.dot
# Root family: you'll need to adjust this most probably.
rootfamily = F1
# Number of cycles to show neighbour families: you can start with 0 and then increase it as you
# like.
familydepth = 4
# Directory containing 100x100px "First Last YYYY.jpg" images.
imagedir = images
